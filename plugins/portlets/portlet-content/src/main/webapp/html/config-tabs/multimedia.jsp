<div class="configContainer singleColumn">
	<div class="first">
		<input name="videoPlayInList" type="checkbox" value="">
		<label>Play Video In List</label>
	</div>
</div>
<div class="configContainer singleColumn">
	<div class="first">
		<input name="videoAutostart" type="checkbox" value="">
		<label>Autostart Video</label>
	</div>
</div>
<div class="configContainer">
	<div class="first">
		<label>Jwplayer Version:</label>
	</div>
	<div class="last">
		<select name="jwplayerVersion" class="inputMediumSize">
			<option value="">Default</option>
			<option value="">5.05</option>
			<option value="">5.10</option>
		</select>
	</div>
</div>
<div class="configContainer">
	<div class="first">
		<label>Video Skin Path</label>
	</div>
	<div class="last">
		<input name="videoSkinPath" type="text" class="inputLongSize">
	</div>
</div>
<div class="configContainer">
	<div class="first">
		<label>Video Canvas Size:</label>
	</div>
	<div class="last">
		<label>Width:</label>
		<input	 name="videoWidh" type="text" class="inputShortestSize">
		<label>Height:</label>
		<input name="videoHeight" type="text" class="inputShortestSize">
	</div>
</div>
<div class="configContainer">
	<div class="first">
		<label>Video PreRoll Id:</label>
	</div>
	<div class="last">
		<input name="videoPreRollId" type="text" class="inputShortSize">
	</div>
</div>
<div class="configContainer">
	<div class="first">
		<label>Video Preview Image Version:</label>
	</div>
	<div class="last">
		<input name="videoPreviewImageVersion" type="text" class="inputMediumSize">
	</div>
</div>